<% if @account.prevalidate_sub.blank? && @account.validate_sub == nil %>

	<%= render 'prevalidate_subdomain', account: @account %>

<% elsif @account.prevalidate_sub == true && @account.validate_sub.blank? %>

	<%= render 'validate_subdomain', account: @account %>

<% elsif @account.prevalidate_sub == true && @account.validate_sub == true && @account.open_check.blank? %>

	<%= render 'already_open', account: @account %>

<% end %>

<hr>

